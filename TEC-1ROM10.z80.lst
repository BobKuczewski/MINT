0586                             ; ********************************************************************************
0586                             ; Number Handling Routine - converts numeric ascii string to a 16-bit number in HL
0586                             ; Read the first character.
0586                             ;			
0586                             ; Number characters ($30 to $39) are converted to digits by subtracting $30
0586                             ; and then added into the L register. (HL forms a 16-bit accumulator)
0586                             ; Fetch the next character, if it is a number, multiply contents of HL by 10
0586                             ; and then add in the next digit. Repeat this until a non-number character is
0586                             ; detected. Add in the final digit so that HL contains the converted number.
0586                             ; Push HL onto the stack and proceed to the dispatch routine.
0586                             ; ********************************************************************************
0586                             ; 
0586                NUMBER:      
0586   21 00 00               LD   HL,$0000   ; 10t Clear HL to accept the number
0589   0A                     LD   A,(BC)   ; 7t  Get the character which is a numeral
058A                NUMBER1:      ; corrected KB 24/11/21
058A   D6 30                  SUB   $30   ; 7t    Form decimal digit
058C   85                     ADD   A,L   ; 4t    Add into bottom of HL
058D   6F                     LD   L,A   ; 4t
058E   3E 00                  LD   A,00   ; 4t    Clear A
0590   8C                     ADC   A,H   ; Add with carry H-reg
0591   67                     LD   H,A   ; Put result in H-reg
0592   03                     INC   BC   ; 6t    Increment IP
0593   0A                     LD   A,(BC)   ; 7t    and get the next character
0594   FE 30                  CP   $30   ; 7t    Less than $30
0596   38 0C                  JR   C,endnum   ; 7/12t Not a number / end of number
0598   FE 3A                  CP   $3A   ; 7t    Greater or equal to $3A
059A   30 08                  JR   NC,endnum   ; 7/12t Not a number / end of number
059C                TIMES10:      ; Multiply digit(s) in HL by 10
059C   29                     ADD   HL,HL   ; 11t    2X
059D   5D                     LD   E,L   ;  4t    LD DE,HL
059E   54                     LD   D,H   ;  4t
059F   29                     ADD   HL,HL   ; 11t    4X
05A0   29                     ADD   HL,HL   ; 11t    8X
05A1   19                     ADD   HL,DE   ; 11t    2X  + 8X  = 10X
05A2                             ; 52t cycles
05A2   18 E6                  JR   number1   
05A4                ENDNUM:      
05A4   E5                     PUSH   HL   ; 11t   Put the number on the stack
05A5   0B                     DEC   BC   
05A6   FD E9                  JP   (IY)   ; and process the next character
05A8                AGAIN:       
05A8   21 1A 0B               LD   HL,vFlags   
05AB   00                     BIT   fELSE,(HL)   
05AC   28 25                  JR   Z,again2   
05AE   DD 5E 00               LD   E,(IX+0)   ; peek loop var
05B1   DD 56 01               LD   D,(IX+1)   
05B4   DD 6E 02               LD   L,(IX+2)   ; peek loop limit
05B7   DD 66 03               LD   H,(IX+3)   
05BA   B7                     OR   A   
05BB   ED 52                  SBC   HL,DE   
05BD   28 0F                  JR   Z,again1   
05BF   13                     INC   DE   
05C0   DD 73 00               LD   (IX+0),E   ; poke loop var
05C3   DD 72 01               LD   (IX+1),D   
05C6   DD 4E 04               LD   C,(IX+4)   ; peek loop address
05C9   DD 46 05               LD   B,(IX+5)   
05CC   FD E9                  JP   (IY)   
05CE                AGAIN1:      
05CE   11 06 00               LD   DE,6   ; drop loop frame
05D1   DD 19                  ADD   IX,DE   
05D3                AGAIN2:      
05D3   FD E9                  JP   (IY)   
