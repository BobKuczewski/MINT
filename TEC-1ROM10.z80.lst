05C6   29                     ADD   HL,HL   ; 11t    2X
05C7   5D                     LD   E,L   ;  4t    LD DE,HL
05C8   54                     LD   D,H   ;  4t
05C9   29                     ADD   HL,HL   ; 11t    4X
05CA   29                     ADD   HL,HL   ; 11t    8X
05CB   19                     ADD   HL,DE   ; 11t    2X  + 8X  = 10X
05CC                             ; 52t cycles
05CC   18 E6                  JR   number1   
