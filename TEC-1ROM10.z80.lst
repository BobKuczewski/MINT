0541   5D                     LD   E,L   ;        LD DE,HL
0542   54                     LD   D,H   ; 
0543   29                     ADD   HL,HL   ;        4X
0544   29                     ADD   HL,HL   ;        8X
0545   19                     ADD   HL,DE   ;        2X  + 8X  = 10X
0546                             ; 52t cycles
0546   18 E6                  JR   num1   
0548                NUM2:        
