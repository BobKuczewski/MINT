0253   2A 0E 0A               LD   HL,(vHeapPtr)   ; load heap ptr
0256   73                     LD   (HL),E   ; store lsb
0257   3A 36 0A               LD   A,(vByteMode)   
025A   23                     INC   HL   
025B   B7                     OR   A   
025C   20 02                  JR   NZ,compNext1   
025E   72                     LD   (HL),D   
025F   23                     INC   HL   
0260                COMPNEXT1:      
0260   22 0E 0A               LD   (vHeapPtr),HL   ; save heap ptr
0263   18 E4                  JR   NEXT   
