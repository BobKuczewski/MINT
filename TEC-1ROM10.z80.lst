0722                             ; Fetch the next character, if it is a number, multiply contents of HL by 10
0722                             ; and then add in the next digit. Repeat this until a non-number character is
0722                             ; detected. Add in the final digit so that HL contains the converted number.
0722                             ; Push HL onto the stack and proceed to the dispatch routine.
0722                             ; ********************************************************************************
0722                NUM:         
0722   21 00 00               LD   hl,$0000   ; Clear hl to accept the number
0725   0A                     LD   a,(bc)   ; Get numeral or -
0726   FE 2D                  CP   "-"   
0728   20 01                  JR   nz,num0   
072A   03                     INC   bc   ; move to next char, no flags affected
072B                NUM0:        
072B   08                     EX   af,af'   ; save zero flag = 0 for later
072C                NUM1:        
072C   0A                     LD   a,(bc)   ; read digit
072D   D6 30                  SUB   "0"   ; less than 0?
072F   38 13                  JR   c,num2   ; not a digit, exit loop
0731   FE 0A                  CP   10   ; greater that 9?
0733   30 0F                  JR   nc,num2   ; not a digit, exit loop
0735   03                     INC   bc   ; inc IP
0736   54 5D                  LD   de,hl   ; multiply hl * 10
0738   29                     ADD   hl,hl   
0739   29                     ADD   hl,hl   
073A   19                     ADD   hl,de   
073B   29                     ADD   hl,hl   
073C   85                     ADD   a,l   ; add digit in a to hl
073D   6F                     LD   l,a   
073E   3E 00                  LD   a,0   
0740   8C                     ADC   a,h   
0741   67                     LD   h,a   
0742   18 E8                  JR   num1   
0744                NUM2:        
0744   0B                     DEC   bc   
0745   08                     EX   af,af'   ; restore zero flag
0746   20 07                  JR   nz,num3   
0748   EB                     EX   de,hl   ; negate the value of hl
0749   21 00 00               LD   hl,0   
074C   B7                     OR   a   ; jump to sub2
074D   ED 52                  SBC   hl,de   
074F                NUM3:        
074F   E5                     PUSH   hl   ; Put the number on the stack
0750   FD E9                  JP   (iy)   ; and process the next character
0752                HEX:         ;=26
0752   21 00 00               LD   HL,0   ; Clear HL to accept the number
0755                HEX1:        
0755   03                     INC   BC   
0756   0A                     LD   A,(BC)   ; Get the character which is a numeral
0757   CB 77                  BIT   6,A   ; is it uppercase alpha?
0759   28 02                  JR   Z,hex2   ; no a decimal
075B   D6 07                  SUB   7   ; sub 7  to make $A - $F
075D                HEX2:        
075D   D6 30                  SUB   $30   ; Form decimal digit
075F   DA 44 07               JP   C,num2   
0762   FE 10                  CP   $0F+1   
0764   D2 44 07               JP   NC,num2   
0767   29                     ADD   HL,HL   ; 2X ; Multiply digit(s) in HL by 16
0768   29                     ADD   HL,HL   ; 4X
0769   29                     ADD   HL,HL   ; 8X
076A   29                     ADD   HL,HL   ; 16X
076B   85                     ADD   A,L   ; Add into bottom of HL
076C   6F                     LD   L,A   ; 
076D   18 E6                  JR   hex1   
