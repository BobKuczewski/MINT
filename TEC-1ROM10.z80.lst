0746                             ; This CFA is updated to point to the character after uppercase alpha
0746                             ; The remainder of the characters are then skipped until after a semicolon
0746                             ; is found.
0746                             ; ***************************************************************************
0746                DEF:         ; Create a colon definition
0746   03                     INC   BC   
0747   0A                     LD   A,(BC)   ; Get the next character
0748   32 8C 0C               LD   (vLastDef),A   
074B   CD 46 03               CALL   lookupRef   
074E   ED 5B 92 0C            LD   DE,(vHeapPtr)   ; start of defintion
0752   73                     LD   (HL),E   ; Save low byte of address in CFA
0753   23                     INC   HL   
0754   72                     LD   (HL),D   ; Save high byte of address in CFA+1
0755   03                     INC   BC   
0756                DEF1:        ; Skip to end of definition
0756   0A                     LD   A,(BC)   ; Get the next character
0757   03                     INC   BC   ; Point to next character
0758   12                     LD   (DE),A   
0759   13                     INC   DE   
075A   FE 3B                  CP   ";"   ; Is it a semicolon
075C   28 02                  JR   Z,def2   ; end the definition
075E   18 F6                  JR   def1   ; get the next element
